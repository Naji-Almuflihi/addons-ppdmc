<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="view_tenant_tenancy_form_inherit" model="ir.ui.view">
          <field name="name">tenant.tenancy.form.inherit</field>
          <field name="model">tenant.tenancy</field>
          <field name="inherit_id" ref="iwesabe_ppmdc_airport_management.view_tenant_tenancy_form" />
          <field name="arch" type="xml">
               <xpath expr="//div[@name='button_box']" position="inside">
                    <button name="action_view_equipment" type="object" class="oe_stat_button" icon="fa-gears">
                         <field name="equipment_count" string="Equipments" widget="statinfo"/>
                    </button>
               </xpath>
          </field>
     </record>     

</odoo>
