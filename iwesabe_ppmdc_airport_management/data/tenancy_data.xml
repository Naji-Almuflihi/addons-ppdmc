<odoo>
    <data >
        <record id="seq_property" model="ir.sequence">
        	<field name="name">Property</field>
        	<field name="code">property</field>
        	<field name="prefix">P/</field>
        	<field name="padding">5</field>
        </record>

		<record id="eq_tenancy" model="ir.sequence">
			<field name="name">Tenant Tenancy</field>
			<field name="code">tenant.tenancy</field>
			<field name="prefix">T/%(year)s</field>
			<field name="padding">5</field>
			<field name="number_next_actual">1</field>
			<field name="number_increment">1</field>
			<field eval="False" name="company_id"/>
		</record>

    </data>
</odoo>
