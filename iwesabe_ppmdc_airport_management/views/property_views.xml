     <!-- Property Search View -->
     <record id="view_property_search" model="ir.ui.view">
          <field name="name">property.search</field>
          <field name="model">property.property</field>
          <field name="arch" type="xml">
               <search string="Property View">
                    <filter string="Active" name="active" domain="[('active','=',1)]"/>
                    <filter string="In Active" name="active" domain="[('active','!=',1)]"/>
                    <filter string="Available" name="available" domain="[('state','=','draft')]"/>
                    <filter string="On Lease" name="on_lease" domain="[('state','=','normal')]"/>
                    <filter string="Booked" name="book" domain="[('state','=','book')]"/>
                    <filter string="Cancel" name="cancel" domain="[('state','=','cancel')]"/>
                    <filter string="Sale" name="sale" domain="[('state','=','close')]"/>
                    <filter string="Sold" name="sold" domain="[('state','=','sold')]"/>
                    <filter string="Parent Property" name="property" domain="[('parent_id','=',False)]"/>
                    <filter string="Child Property" name="property" domain="[('parent_id','!=',False)]"/>
                    <field name="name"/>
                    <field name="property_type_id"/>
                    <field name="parent_id"/>
                    <field name="furnished"/>
                    <field name="acquisition_date"/>
                    <newline/>
                    <group expand="0" string="Group By..." colspan="4" col="10">
                         <filter string="Parent Property" name="parentproperty" icon="terp-personal" domain="[]" context="{'group_by':'parent_id'}"/>
                         <filter name="property_type" string="Property Type" icon="terp-personal" domain="[]" context="{'group_by':'property_type_id'}"/>
                         <filter name="furnish_type" string="Furnish Type" icon="terp-personal" domain="[]" context="{'group_by':'furnished'}"/>
                        

                         <filter string="State" name="grpstate" icon="terp-personal" domain="[]" context="{'group_by':'state'}"/>
                         <filter string="Country" name="country" icon="terp-personal" domain="[]" context="{'group_by':'country_id'}"/>
                         <filter name="acquisition_date" string="Date" context="{'group_by':'acquisition_date'}"/>
                    </group>
               </search>
          </field>
     </record>

     <!-- Property Kanban View -->
     <record id="property_kanban_view" model="ir.ui.view">
          <field name="name">Property Kanban</field>
          <field name="model">property.property</field>
          <field name="arch" type="xml">
               <kanban default_group_by="state">
                    <field name="state"/>
                    <field name="color"/>
                    <field name="name"/>
                    <field name="id"/>
                    <field name="parent_id"/>
                    <field name="property_type_id"/>
                    <field name="furnished"/>
                    <field name="country_id"/>
                    <field name="state_id"/>
                    <!-- <field name="image_medium"/> -->
                    <field name="image"/>
                    <!-- <field name="image"/> -->
                    <templates>
                         <t t-name="kanban-box">
                              <div t-attf-class="oe_kanban_color_#{kanban_getcolor(record.color.raw_value)} oe_kanban_vignette oe_semantic_html_override oe_kanban_card oe_kanban_project oe_kanban_global_click">
                                   <table width="100%">
                                        <tr>
                                             <td width="30%">
                                                  <a type="open">
                                                       <t t-if="record.image">
                                                            <div class="o_kanban_image">
                                                                 <img t-att-alt="record.name.raw_value" t-att-src="kanban_image('property.property', 'image', record.id.value)" style="width:100% !important;height:150% !important;"/>
                                                            </div>
                                                       </t>
                                                       <t t-else="">
                                                            <div class="o_kanban_image">
                                                                 <img t-att-alt="record.name.raw_value" t-att-src='_s + "/base/static/img/company_image.png"' style="width:100% !important;height:150% !important;"/>
                                                            </div>
                                                       </t>
                                                  </a>
                                             </td>
                                             <td width="67%">
                                                  <div class="oe_kanban_details">
                                                       <h5 class="oe_partner_heading" style="padding-right: 20px;">
                                                            <a type="open">
                                                                 <field name="name"/>
                                                            </a>
                                                       </h5>
                                                       <ul>
                                                            <li t-if="record.property_type_id.raw_value ">
                                                                 <b>Type :
                                                                 </b>
                                                                 <field name="property_type_id"/>
                                                            </li>
                                                            <li t-if="record.furnished.raw_value">
                                                                 <b>Furnishing :
                                                                 </b>
                                                                 <field name="furnished"/>
                                                            </li>
                                                       </ul>
                                                  </div>
                                             </td>
                                             <td width="3%">
                                                  <div t-attf-class="oe_kanban_color_#{kanban_getcolor(record.color.raw_value)} oe_kanban_card oe_kanban_global_click">
                                                       <div class="o_dropdown_kanban dropdown" groups="base.group_user">
                                                            <a class="dropdown-toggle btn" role="button" data-toggle="dropdown" href="#">
                                                                 <span title="menu_toggle" class="fa fa-bars fa-lg"/>
                                                            </a>
                                                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                                                 <t t-if="widget.editable">
                                                                      <li>
                                                                           <a type="edit">Edit</a>
                                                                      </li>
                                                                 </t>
                                                                 <t t-if="widget.deletable">
                                                                      <li>
                                                                           <a type="delete">Delete</a>
                                                                      </li>
                                                                 </t>
                                                                 <li>
                                                                      <ul class="oe_kanban_colorpicker" data-field="color"/>
                                                                 </li>
                                                            </ul>
                                                       </div>
                                                  </div>
                                             </td>
                                        </tr>
                                   </table>
                              </div>
                         </t>
                    </templates>
               </kanban>
          </field>
     </record>
    
