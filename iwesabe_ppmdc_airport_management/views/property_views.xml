                         <!-- <button name="validate" states="draft" string="Confirm" type="object" class="oe_highlight"/> -->
                         <button type="object" name="compute_depreciation_board" class="oe_highlight" string="Compute Depreciation" states="draft" invisible='1'/>
                         <button name="action_set_to_close" string="Sell or Dispose" type="object" class="oe_highlight" attrs="{'invisible': [('state', '!=', 'open')]}"/>
                         <button name="set_to_draft" string="Set to Draft" type="object" attrs="{'invisible': [('state', '!=', 'open')]}"/>
                         <button name="set_to_running" string="Set to Running" type="object" attrs="{'invisible': [('state', '!=', 'close')]}"/>
                         <button name="action_asset_pause" string="Pause Depreciation" type="object" attrs="{'invisible': [('state', '!=', 'open')]}" invisible='1'/>
                         <button name="resume_after_pause" string="Resume Depreciation" type="object" attrs="{'invisible': [('state', '!=', 'paused')]}" invisible='1'/>
                         <button name="action_asset_modify" states="open" string="Modify Depreciation" type="object" invisible='1'/>
                         <button name="action_save_model" states="open,paused,close" string="Save Model" type="object" />
                         
                         <button name="button_normal" string="Open" type="object" states='draft' icon="fa-folder-open" colspan="2"/>
                         <field name="state" widget="statusbar" statusbar_visible="draft,book,normal,cancle" />
                    </header>
                    <sheet>
                         <div class="oe_button_box" name="button_box">
                             
                              <button name="button_cancel" string="End of Cycle" type="object" states='normal,sold,close' icon="fa-undo" colspan="2" class="oe_stat_button"/>
                              <button name="button_draft" string="Set to Available" type="object" states='cancel' icon="fa-home" colspan="2" class="oe_stat_button"/>
                         
                              <button class="oe_stat_button" colspan="2" name="open_assets" type="object" icon="fa-pencil">
